{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "phenotype",
  "description": "CTTV phenotype model",
  "type": "object",
  "version": "1.2",
  "properties": {
    "about": {
      "allOf": [
        {
          "$ref": "https://raw.githubusercontent.com/CTTV/json_schema/master/src/bioentity/bioentities.json"
        }
      ],
      "items": {
        "description": "An array of phenotype identifers",
        "pattern": "^http://purl.obolibrary.org/obo/HP_[0-9]{4,}||http://purl.obolibrary.org/obo/MP_[0-9]{4,}$"
      },
      "minItems": 1
    }
  },
  "required": [
    "about"
  ],
  "additionalProperties": false
}