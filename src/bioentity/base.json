{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Bioentities",
    "description": "A generic schema for an array of base. Must be subclassed with additional properties to be meaningful.",
    "version": "1.2",
    "type": "object",
    "id": "#base_bioentity",
    "properties" : {
      "id" : {
        "type" : "array",
        "id" : "#id",
        "items": {
            "type": "string"
        },
        "minItems": 1,
        "uniqueItems": true
      }
    }
}
