{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "snp_gwas_disease_evidence",
  "description": "Evidence linking SNP to disease in a GWAS study",
  "type": "object",
  "version": "1.2",
  "allOf": [
    {
      "$ref": "https://raw.githubusercontent.com/CTTV/json_schema/master/src/evidence/root.json"
    }
  ],
  "properties": {
    "evidence_codes": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "enum": [
          "http://identifiers.org/eco/GWAS",
          "http://purl.obolibrary.org/obo/ECO_0000205"
        ]
      }
    }
  },
  "additionalProperties": false
}