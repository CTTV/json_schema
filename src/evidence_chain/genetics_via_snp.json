{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Genetics evidence chain via SNP association",
  "description": "CTTV genetics evidence chain where there is SNP evidence",
  "type": "object",
  "version": "1.2",
  "allOf": [
    {
      "$ref": "https://raw.githubusercontent.com/CTTV/json_schema/master/src/evidence_chain/root.json"
    }
  ],
  "properties": {
    "type_of": {
      "type": "string",
      "default": "genetics_via_snp"
    },
    "chain": {
      "type": "array",
      "items": [
        {
          "$ref": "https://raw.githubusercontent.com/CTTV/json_schema/master/src/bioentity/target.json"
        },
        {
          "$ref": "https://raw.githubusercontent.com/CTTV/json_schema/master/src/evidence/genetics/snp.json"
        },
        {
          "$ref": "https://raw.githubusercontent.com/CTTV/json_schema/master/src/bioentity/snp.json"
        },
        {
          "$ref": "https://raw.githubusercontent.com/CTTV/json_schema/master/src/evidence/genetics/gwas.json"
        },
        {
          "$ref": "https://raw.githubusercontent.com/CTTV/json_schema/master/src/bioentity/disease.json"
        }
      ],
      "additionalItems": false
    }
  },
  "remote_schemas": {
  	"bioentities": {
  		"$ref": "https://raw.githubusercontent.com/CTTV/json_schema/master/src/bioentity/bioentities.json"
  	},
  	"evidence": {
  		"$ref": "https://raw.githubusercontent.com/CTTV/json_schema/master/src/evidence/root.json"
  	}
  }
}